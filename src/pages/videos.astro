import VideoList from "@/components/VideoList.astro";

export const prerender = true;
---

<Page
  title={`Videos | ${SITE_TITLE}`}
  description="Videos I've made"
  keyword="continue"
>
  <Main title="Videos">
    <VideoList grid={true} />
  </Main>
</Page>
